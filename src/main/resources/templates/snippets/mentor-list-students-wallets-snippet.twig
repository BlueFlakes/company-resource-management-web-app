<table>
    <thead>
        <tr>
            <th>Student id</th>
            <th>Name</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
        {% for student in students %}
        <tr>
            <td>{{ student.getId() }}</td>
            <td>{{ student.getName() }}</td>
            <td>{{ student.getPossesedCoins() }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>